pico-8 cartridge // http://www.pico-8.com
version 27
__lua__

function _init()
-- enable mouse
 poke(0x5f2d, 1)
 reset()
end

function _draw()
 cls()

 draw_grid()

 for num = 0,8 do
   draw_square(num)
 end

 if (p[squarenum] == "") then
  draw_nought(grid_to_coord(mousecol), grid_to_coord(mouserow))
 end

 if (finished) then
   rectfill(28, 50, 100, 78, 2)
   color(6)
   print("finished", 48, 58)

   if (winner != "") then
     print(""..winner.." is the winner!", 32, 70)
   else
     print("game was a draw.", 30, 70)
   end
 end

 draw_cursor(mousex, mousey, 4)
end

function draw_cursor(x, y, size)
 line(x, y - size, x, y + size)
 line(x - size, y, x + size, y)
end

function draw_grid()
 line(42, 0, 42, 127)  -- two vertical lines
 line(85, 0, 85, 127)

 line(0, 42, 127, 42)  -- two horizontal lines
 line(0, 85, 127, 85)
end

function draw_square(num)
 local x = grid_to_coord(num % 3)
 local y = grid_to_coord(flr(num / 3))
 
 if (p[num + 1] == "0") then
  draw_nought(x, y)
 end

 if (p[num + 1] == "x") then
  draw_cross(x, y)
 end
end

function draw_nought(x, y)
 circ(x, y, 10)
end

function draw_cross(x, y)
 line(x - 10, y - 10, x + 10, y + 10)
 line(x + 10, y - 10, x - 10, y + 10)
end

function grid_to_coord(grid)
 return (42.666 * (grid + 1)) - (42.666 / 2)
end

function cpu_move()
  local empty = 0

  for num = 0,8 do
   if (p[num + 1] == "") then
    empty = empty + 1
   end
  end

  target = flr(rnd(empty)) + 1
  empty = 0

  for num = 0,8 do
   if (p[num + 1] == "") then
    empty = empty + 1

    if (empty == target) then
      p[num + 1] = "x"
      return
    end
   end
  end
end

function check_for_win()
 for a = 1,3 do
   // columns 1, 2, and 3
   if p[a] != "" and p[a] == p[a + 3] and p[a] == p[a + 6] then
     finished = true
     winner = p[a]
   end

   // rows 1, 2, and 3
   b = (a - 1) * 3

   if p[b + 1] != "" and p[b + 1] == p[b + 2] and p[b + 1] == p[b + 3] then
     finished = true
     winner = p[b + 1]
   end
 end

// diagonal 1
 if p[1] != "" and p[1] == p[5] and p[1] == p[9] then
   finished = true
   winner = p[1]
 end

// diagonal 2
 if p[3] != "" and p[3] == p[5] and p[3] == p[7] then
   finished = true
   winner = p[3]
 end

 draw = true

 for c = 1,8 do
   if p[c] == "" then
     draw = false
     break
   end
 end

 if finished != true and draw then
   finished = true
 end
end

function reset()
  p = {"", "", "", "", "", "", "", "", ""}
  finished = false
  winner = ""
  delay = 0
end

function _update()
 if delay > 0 then
  delay = delay - 1

  if (delay <= 0) then
    reset()
  end
 end

 mousex = stat(32)
 mousey = stat(33)

 mousecol=flr(mousex/42.666)
 mouserow=flr((mousey/42.666) % 3)

 squarenum = ((mouserow * 3) + mousecol) + 1

 if (down and stat(34) == 0) then
  down = false
 end

 if ((not down) and stat(34) == 1) then
  down = true

  if (p[squarenum] == "") then
    p[squarenum] = "0"
    check_for_win()

    if (not finished) then
     cpu_move()
     check_for_win()
    end

    if (finished and delay <= 0) then
      delay = 60
    end
  end
 end
end
